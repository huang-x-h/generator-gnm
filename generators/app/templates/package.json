{
  "name": "<%= moduleName %>",
  "version": "0.1.0",
  "description": "<%= moduleDescription %>",
  <% if (cli) { %>"bin": {
    "<%= commandName %>": "./cli.js"
  },<% } %>
  "main": "index.js",
  "files": ["index.js"],
  "repository": {
    "type": "git",
    "url": "git://github.com/<%= authorName %>/<%= moduleName %>.git"
  },
  "homepage": "https://github.com/<%= authorName %>/<%= moduleName %>",
  "keywords": [
  ],
  "license": "MIT",
  "author": {
    "name": "<%= fullName %>",
    "email": "<%= emailAddress %>",
    "url": "<%= website %>"
  },
  "bugs": {
    "url": "https://github.com/<%= authorName %>/<%= moduleName %>/issues"
  }
}
